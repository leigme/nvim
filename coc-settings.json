{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod"
      ]
      "filetypes": [
        "go"
      ]
    }
  },
  {
    "languageserver": {
      "rust": {
        "command": "rust-analyzer",
        "filetypes": ["rust"],
        "rootPatterns": ["Cargo.toml"]
      }
    }
  },
  "suggest.noselect": false,
  "coc.preferences.diagnostic.displayByAle": true,
  "suggest.floatEnable": true
}
